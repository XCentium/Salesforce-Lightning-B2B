                            <c:FIDO_IfContains items="{!feature.name}"  element="{!v.SearchPhrase}"> 
                                <aura:if isTrue="{!feature.demo_org_feature__c == null }">
                                    <div class="featurewrapper">
                                        <div class="featureitem featureleft">
                                            <div class="padding">
                                                <strong class="featuretitle" >{!feature.name}</strong>
                                                <aura:if isTrue="{!feature.description_of_customization__c != undefined}">
                                                    <div class="featureinfoicon">
                                                        <lightning:icon iconName="utility:info" size="xx-small" alternativeText=""  /> 
                                                        <div class="implementationDetails" aura:id="{!feature.Id}">
                                                            <div class="padding center">
                                                                <ui:outputRichText aura:id="outputRT" value="{!feature.description_of_customization__c}" />
                                                            </div>
                                                        </div>
                                                    </div>
                                                </aura:if>
                                            </div>
                                        </div>
                                        <c:FIDO_FeatureCompareOrgCheck FeatureSFID="{!feature.sfid}" recid="{!v.recid}" ScreenChoice="{!v.ScreenChoice}" OrgFeatures="{!v.OrgFeatures}" FeatureName="{!feature.name}" DemoOrg="{!v.DemoOrg}" />
                                        <div class="clear"></div>
                                    </div>
                                    <div class="clear"></div>
                                    <aura:iteration items="{!v.Features}" var="Subfeature"> 
