<aura:component controller="FIDO">
    <aura:attribute name="Features" type="String[]"/>
    <aura:attribute name="Industry" type="String"/>
    <aura:attribute name="OrgFeatures" type="String[]"/>
    <aura:attribute name="Org2Features" type="String[]"/>
    <aura:attribute name="DemoOrg" type="String" />
    <aura:attribute name="OrgId" type="String" />
    <aura:attribute name="DemoOrg2" type="String" default="" />
    <aura:attribute name="OrgIdentifier" type="String" />
    <aura:attribute name="OrgIdentifier2" type="String" />
    <aura:attribute name="Org2Name" type="String" />
    <aura:attribute name="OrgName" type="String" />
    <aura:attribute name="orgOptions" type="List"/>
    <aura:attribute name="IndustryName" type="String" />
    <aura:attribute name="ScreenChoice" type="String" default="{!v.ScreenChoice}" />
    <aura:attribute name="SearchPhrase" type="String" default='' />
    <aura:attribute name="SearchType" type="String" /> 
    <aura:attribute name="recid" type="String" />
    <aura:handler name="init" value="{!this}" action="{!c.init}"/>
    <aura:attribute name="Spinner" type="boolean" default="false"/>
    <aura:if isTrue="{!and(v.SearchPhrase != '',v.SearchType == 'Features')}">
        <div class="paddingleftright">
            <h3 class="searchHeader">Search Results for: {!v.SearchPhrase}</h3>
        </div>
    </aura:if>
    <aura:if  isTrue="{!or(v.recid == '', v.recid == null)}" >
        <div class="owner greenbordertop">
            <h3 class="borderbottom">
                <div class="padding">
                    <lightning:icon iconName="standard:resource_preference" size="medium" alternativeText=""/>   
                    <span class="marginleft">What Features are in Your IDO</span>
                </div>
            </h3>
            <div class="textleft padding">
                <aura:if isTrue="{!v.SearchPhrase != ''}">
                    <div class="featurelist">
                        <div class="featurewrapper">
                            <div class="featureitem title"><div class="padding"></div></div>
                            <div class="featureitem  tableheader">
                                <div class="margin10 center ">
                                    <div class="title2 paddingsmall">
                                        <strong>Enabled</strong>
                                    </div>
                                </div>
                            </div> 
                            <div class="featureitem  tableheader">
                                <div class="margin10 center ">
                                    <div class="title2 paddingsmall">
                                        <strong>Customization</strong>
                                    </div>
                                </div>
                            </div> 
                            <div class="clear"></div>
                        </div>
                    </div>
